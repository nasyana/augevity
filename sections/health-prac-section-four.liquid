<section class="Section Section--spacingNormal nase-three-boxes withimage desktop-only" id="section-{{ section.id }}">
    <div class="Container">
      {%- if section.settings.subheading != blank or section.settings.title != blank -%}
        <header class="SectionHeader SectionHeader--center">
          {%- if section.settings.subheading != blank -%}
            <h3 class="SectionHeader__SubHeading Heading u-h6"></h3>
          {%- endif -%}
  
          {%- if section.settings.title != blank -%}
            <h2 class="SectionHeader__Heading Heading u-h1">{{ section.settings.title | escape }}</h2>
          {%- endif -%}
        </header>
      {%- endif -%}
  
      <div class="Rte" style="text-align: center">
        {{ section.settings.html }}
      </div>
    </div>
    
    <div id="threeboxcontainer" class="withimage">
  <div id="threeboxes">
  <div class="nasleft nasbox">
    
   <img id="nasicon" src="{{ section.settings.item-image1 | img_url: '2048x655' }}" style="width: 100%">
  <h4>{{ section.settings.box1subhead | escape }}</h4>
  <p>{{ section.settings.box1p | escape }}</p>
  </div>
  <div class="nascenter nasbox noborder">
     <img id="nasicon" src="{{ section.settings.item-image2 | img_url: '2048x655' }}" style="width: 100%">
    
  <h4>{{ section.settings.box2subhead | escape }}</h4>
  <p>{{ section.settings.box2p | escape }}</p>
  </div>
  
  <div class="nasright nasbox">
       <img id="nasicon" src="{{ section.settings.item-image3 | img_url: '2048x655' }}" style="width: 100%">
  <h4>{{ section.settings.box3subhead | escape }}</h4>
  <p>{{ section.settings.box3p | escape }}</p>
  </div>
  </div>
  </div>
    
    
  </section>
  
  {% schema %}
  {
    "name": "Health Prac Four",
    "class": "shopify-section--bordered",
    "settings": [
                  {
                    "type": "image_picker",
                    "id": "item-image1",
                    "label": "Icon Image"
                  },
      {
        "type": "text",
        "id": "box1subhead",
        "label": "Box 1 Sub Heading",
        "default": "Sub-heading"
      },
      {
        "type": "text",
        "id": "box1p",
        "label": "Heading",
        "default": "Text"
      },{ "type": "image_picker",
          "id": "item-image2",
          "label": "Icon Image"
                  },{
        "type": "text",
        "id": "box2subhead",
        "label": "Box 2 Sub Heading",
        "default": "Sub-heading"
      },{
        "type": "text",
        "id": "box2p",
        "label": "Heading",
        "default": "Text"
      },{"type": "image_picker",
         "id": "item-image3",
         "label": "Icon Image"
                  },{
        "type": "text",
        "id": "box3subhead",
        "label": "Box 3 Sub Heading",
        "default": "Sub-heading"
      },
      {
        "type": "text",
        "id": "box3p",
        "label": "Heading",
        "default": "Text"
      }
      
    ],
    "presets": [
      {
        "category": "Advanced",
        "name": "Three Boxes with Image",
        "settings": {}
      }
    ]
  }
  {% endschema %}