<style>
    @media only screen and (max-width: 500px) {
  
      #onlymobile{display: block;}
      #onlydesktop{display: none;}
  }
      @media only screen and (min-width: 500px) {
  
      #onlymobile{display: none;}
      #onlydesktop{display: block;} 
  } 
  </style>  
   {% for block in section.blocks %}
  <section class="full_width about_section_six wholesaler" style="background: #f57f35;">
  
   
      <div class="full_width onlydesktop" > 
  
        
       
               <h2>{{ block.settings.header }}</h2>

               <p>{{ block.settings.description }}</p>

               <a href="{{ block.settings.block_link }}"  target="_blank"class="aboutbutton">{{ block.settings.buttontext }}</a>
        
      </div>
  
  
  </section>
  {% endfor %}
  
  {% schema %}
  {
      "name": "Wholesaler Section Five",
      "blocks": [
              {
                  "type": "image",
                  "name": "Wholesaler Section Five",
                  "settings": [
                    {
                      "type": "image_picker",
                      "id": "item-image",
                      "label": "Image of the product"
                    }, {
                      "type": "image_picker",
                      "id": "mob-item-image",
                      "label": "Mobile Version"
                    }, {
                      "type": "url",
                      "id": "block_link",
                      "label": "Link URL"
                    }, {
                      "type": "text",
                      "id": "header",
                      "label": "Box Header"
                    },{
                        "type": "text",
                        "id": "buttontext",
                        "label": "Button Text"
                      },{
                        "type": "text",
                        "id": "description",
                        "label": "description"
                      }
  
  
  
  
               
                  ]
              }
      ]
  }
  {% endschema %}