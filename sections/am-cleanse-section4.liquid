<section class="sec-04 gevity_new_style_difference" style="padding: 60px 0;">
    <div class="row" style="padding: 20px 0;">
        <div class="container">
            <div class="col-sm-12 col-md-12 col-lg-8 col-lg-offset-2 text-center">
                <h2 class="newFont">{{ section.settings.main-header }}</h2>
                <p>{{ section.settings.main-description }}</p>
            </div>
        </div>
    </div>


     <div class="row">
          <div class="container">
            <div class="col-sm-12 col-md-4 col-lg-3 col-lg-offset-1 text-center border-right">
    
            
                <img style="margin: 20px 0;" src="{{ section.settings.image-one | img_url: '2048x1055' }}" alt="10x More"/>
            </div>
             <div class="col-sm-12 col-md-4 col-lg-3 text-center">
    
        
                <img style="margin: 20px 0;" src="{{ '39cups.gif' | asset_img_url: '300x' }}" alt="39 Cups"/>
            </div>
            <div class="col-sm-12 col-md-4 col-lg-3 text-center">
    
       
                <img style="margin: 20px 0;" src="{{ '9litres.gif' | asset_img_url: '300x' }}" alt="9 Litres"/>
            </div>
          </div>
        </div>


    <div class="row">
        <div class="container">
            <div class="col-sm-6 col-md-2 col-lg-2 col-lg-offset-2 col-md-offset-2 text-center">
                <img src="{{ section.settings.item1-image | img_url: '2048x1055' }}" alt="Australian Owned"/>
            </div>
            <div class="col-sm-6 col-md-2 col-lg-2 text-center">
                <img src="{{ section.settings.item2-image | img_url: '2048x1055' }}" alt="Gluten Free"/>
            </div>
            <div class="col-sm-6 col-md-2 col-lg-2 text-center">
                <img src="{{ section.settings.item3-image | img_url: '2048x1055' }}" alt="Dairy Free"/>
            </div>
            <div class="col-sm-6 col-md-2 col-lg-2 text-center">
                <img src="{{ section.settings.item4-image | img_url: '2048x1055' }}" alt="Paleo & Allergen Friendly"/>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="container">
            <div class="col-sm-6 col-md-2 col-lg-2 col-lg-offset-2 col-md-offset-2 text-center">
                <img src="{{ section.settings.item5-image | img_url: '2048x1055' }}" alt="Australian Grass Fed Beef"/>
            </div>
            <div class="col-sm-6 col-md-2 col-lg-2 text-center">
                <img src="{{ section.settings.item6-image | img_url: '2048x1055' }}" alt="Practitioner Preferred"/>
            </div>
            <div class="col-sm-6 col-md-2 col-lg-2 text-center">
                <img src="{{ section.settings.item7-image | img_url: '2048x1055' }}" alt="Transparent Labelling"/>
            </div>
            <div class="col-sm-6 col-md-2 col-lg-2 text-center">
                <img src="{{ section.settings.item8-image | img_url: '2048x1055' }}" alt="Concentrated Not Dehydrated"/>
            </div>
            {% comment %} <div class="col-sm-12 text-center">
                <button class="btn btn-warning" type="button">
                    <a href="{{ section.settings.section-url }}">{{ section.settings.button-text }}</a>
                </button>
            </div> {% endcomment %}
        </div>
    </div>
</section>


